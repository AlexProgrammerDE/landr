<nav class="navbar {{ class }}">
  <div class="container">
    <a class="navbar-brand d-inline-block align-middle" href="/">
      {{#if image}}
        <img src="{{ image }}" alt="{{ title }}">
      {{else}}
        {{ title }}
      {{/if}}
    </a>
    <button class="navbar-toggler hidden-md-up float-xs-right" type="button" data-toggle="collapse" data-target="#exCollapsingNavbar">
    </button>
    <div class="collapse navbar-toggleable-sm float-xs-none float-md-right" id="exCollapsingNavbar">
      <ul class="nav navbar-nav">
        {{#each items}}
          <li class="nav-item">
            {{#if this.href }}
              {{> link text=this.text href=this.href class="nav-link" }}
            {{ else }}
              {{{ this }}}
            {{/if}}
          </li>
        {{/each}}
      </ul>
    </div>
  </a>
</nav>
