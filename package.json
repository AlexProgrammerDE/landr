{
  "name": "landr",
  "version": "5.22.0",
  "description": "Generate great websites for your projects with zero configuration",
  "main": "bin/cli.js",
  "bin": {
    "landr": "./bin/cli.js"
  },
  "scripts": {
    "lint": "eslint --ext .js,.jsx bin lib test scripts && csslint lib/**/*.css",
    "unit": "nyc --exclude scripts --exclude test --reporter=html --extension .js --extension .jsx --compact=false ava",
    "build": "./bin/cli.js build",
    "test-mixpanel": "LANDR_MIXPANEL_TOKEN=04ad3098829fcd7c5700d75cea220530 ./bin/cli.js build",
    "test-npm": "rm -f land-*.tgz && npm pack && npm install --global ./landr-$npm_package_version.tgz && landr build && rm landr-*.tgz",
    "test": "npm run lint && npm run unit && npm run build && npm run test-npm && npm run test-mixpanel",
    "start": "npm run build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/balena-io/landr.git"
  },
  "keywords": [
    "website",
    "generation",
    "docs",
    "project",
    "ssg"
  ],
  "author": "Balena.io <hello@balena.io>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/balena-io/landr/issues"
  },
  "homepage": "https://github.com/balena-io/landr",
  "ava": {
    "verbose": true,
    "failWithoutAssertions": true,
    "files": [
      "test/**/*.spec.js",
      "test/**/*.spec.jsx"
    ],
    "require": [
      "./scripts/babel-test-register.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@storybook/addon-a11y": "^5.1.11",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-links": "^5.1.11",
    "@storybook/addons": "^5.1.11",
    "@storybook/react": "^5.1.11",
    "ava": "^2.1.0",
    "babel-loader": "^8.0.6",
    "csslint": "^1.0.5",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsdoc": "^8.3.0",
    "eslint-plugin-lodash": "^5.1.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-standard": "^4.0.0",
    "js-yaml": "^3.13.1",
    "jsdom": "^15.1.1",
    "nyc": "^14.1.1",
    "puppeteer": "^1.20.0"
  },
  "dependencies": {
    "@babel/register": "^7.4.0",
    "@reach/router": "^1.2.1",
    "bluebird": "^3.5.5",
    "chalk": "^2.4.2",
    "circular-std": "^1.0.0",
    "date-fns": "^2.0.1",
    "file-type": "^12.0.1",
    "generatorics": "^1.1.0",
    "git-branch": "^2.0.1",
    "gitinfo": "2.2.0",
    "handlebars": "^4.1.2",
    "handlebars-helpers": "^0.10.0",
    "lodash": "^4.17.11",
    "markdown": "^0.5.0",
    "mixpanel-browser": "^2.29.0",
    "netlify": "^2.4.6",
    "node-fetch": "^2.6.0",
    "node-vibrant": "^3.1.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-static": "~7.1.0",
    "react-static-plugin-reach-router": "^7.0.10",
    "react-static-plugin-sitemap": "^7.0.10",
    "react-static-plugin-styled-components": "^7.0.10",
    "react-typist": "^2.0.5",
    "recursive-copy": "^2.0.10",
    "rendition": "^11.21.0",
    "rfdc": "^1.1.4",
    "rimraf": "^2.6.3",
    "scrutinizer": "^1.23.1",
    "shelljs": "^0.8.3",
    "styled-components": "^4.3.2",
    "tmp": "^0.1.0",
    "typed-errors": "^1.1.0",
    "typeface-nunito": "0.0.72"
  }
}
